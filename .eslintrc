{
    "extends": "airbnb/base",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "DEBUG": false,
    },
    "parser": "babel-eslint",
    "rules": {
        "semi": 0,
	"new-cap": 0,
	"no-param-reassign": 0,
	"no-console": 0,
	"no-restricted-syntax": 0,
    }
}
